#include < stdio.h >  
  
int main()  
{  
    int s1, s2, s3, s4, s5, per;  
  
    printf("Enter marks of 5 subjects\n");  
    scanf("%d %d %d %d %d", &s1, &s2, &s3, &s4, &s5);  
  
    per = (s1 + s2 + s3 + s4 + s5) / 5.0;  
  
    if(per > 100)  
        printf("You've entered wrong marks, kindly re-enter\n");  
    else if(per >= 60)  
        printf("Grade A\n");  
    else if(per >= 50)  
        printf("Grade B\n");  
    else if(per >= 40)  
        printf("Grade C\n");  
    else  
        printf("Failed!\n");  
  
    return 0;  
}  
